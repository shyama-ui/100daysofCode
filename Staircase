public class Solution {

    // Complete the staircase function below.
    static void staircase(int n) {
    int h=1;
    int space=n-1;
    while(n>0)
    {
        for(int i=0;i<space;i++)
        {
            System.out.print(" ");
        }
        for(int i=0;i<h;i++)
        {
            System.out.print("#");
        }
        h=h+1;
        space=space-1;
        n--;
        System.out.println("");
    }


    }
